{
  "expo": {
    "name": "TEDIE",
    "slug": "tedie-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "packagerOpts": {
      "config": "metro.config.js",
      "sourceExts": [
        "expo.ts",
        "expo.tsx",
        "expo.js",
        "expo.jsx",
        "ts",
        "tsx",
        "js",
        "jsx",
        "json",
        "wasm",
        "svg"
      ]
    },
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#d70d0f"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "userInterfaceStyle": "light",
      "bundleIdentifier": "com.tedie.tedie",
      "buildNumber": "2",
      "infoPlist": {
        "NSLocationAlwaysUsageDescription": "Vamos usar a localização para indicar os estabelecimentos próximos ao usuário",
        "NSLocationWhenInUseUsageDescription": "Vamos utilizar quando o usuário acessar o app e também caso o usuário entre na tela de endereços"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "android": {
      "package": "com.tedie.tedie",
      "versionCode": 4,
      "userInterfaceStyle": "light",
      "permissions":["ACCESS_COARSE_LOCATION"]
    },
    "description": "v2"
  }
}
